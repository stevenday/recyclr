<?xml version="1.0" encoding="UTF-8"?>

<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">

    <servlet>
		<servlet-name>FetchFreecycleFeedServlet</servlet-name>
		<servlet-class>uk.co.gifteconomy.FetchFreecycleFeedServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FetchFreecycleFeedServlet</servlet-name>
		<url-pattern>/cron/fetchfreecyclefeed</url-pattern>
	</servlet-mapping>
	
</web-app>
